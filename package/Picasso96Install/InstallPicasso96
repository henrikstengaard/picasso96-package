			(set #Uninstall		"Désinstaller Picasso96")
			(set #UninstallSel	"Que voulez-vous désinstaller ?")
			(set #UninstallSelHelp
				(cat
					"Vous pouvez sélectionner quels sont les modules que vous voulez supprimer."
				)
			)
			(set #AskUninstall	"Voulez-vous vraiment supprimer les options sélectionnées ?")
			(set #AskUnyes			"Oui !")
			(set #AskUnno			"Non !")
			(set #AskUninstallHelp
				(cat
					"Si l'installer trouve des backups de veilles installations avant Picasso96,"
					"il les réinstallera."
				)
			)
	
			(set #InstEnd
				(cat
					"\nL'installation est terminée. L'installer a normalement copié tous les composants à leur place.\n\n"
					"Si vous avez, malgré tout, des problèmes, consultez les pages web suivantes pour obtenir de l'aide :\n\n"
					"http://picasso96.home.pages.de/\n"
					"http://www.villagetronic.com/amiga/\n\nMerci encore d'avoir choisi Picasso96!\n\nL'équipe de Picasso96...."
				)
			)
			(set #UpdEnd
				(cat
					"\nLa procédure de mise à jour est terminée. Le système doit normalement continuer à fonctionner normalement.\n\n"
					"Si vous avez, malgré tout, des problèmes, consultez les pages web suivantes pour obtenir de l'aide :\n\n"
					"http://picasso96.home.pages.de/\n"
					"http://www.villagetronic.com/amiga/\n\nMerci encore d'avoir choisi Picasso96!\n\nL'équipe de Picasso96...."
				)
			)
			(set #UniEnd
				(cat
					"\nLa procédure de désinstallation est terminée et les modules sélectionnés doivent être supprimés.\n\n"
					"Si vous avez, malgré tout, des problèmes, consultez les pages web suivantes pour obtenir de l'aide :\n\nhttp://picasso96.home.pages.de/\n"
					"http://www.villagetronic.com/amiga/\n\nMerci encore d'avoir essayé Picasso96! Et nous espérons que vous le réinstallerez bientôt.\n\n"
					"L'équipe de Picasso96...."
				)
			)
	
		; Environment Variables
			(set #CreateEnvVars "Creating Picasso96 environment variables with default settings.")
			(set #CreateEnvVarsHelp "The environment variables are used to configure some run time features.")
	
		; Boards
			(set #AskBoards	"Quel(s) type(s) de carte(s) graphique(s) désirez-vous utiliser avec Picasso96 ?")
			(set #AskBoardsUp	"Pour quel(s) type(s) de carte(s) graphique(s) désirez-vous faire une mise à jour ?")
			(set #AskBoardsUn	"Pour quel(s) type(s) de carte(s) graphique(s) désirez-vous supprimer le pilote ?")
			(set #AskBoardsHelp
				(cat
					"Sélectionnez les cartes que vous possédez. Dans le cas où elles n'apparaissent pas dans la liste, "
					"vous ne pouvez pas utiliser Picasso96. Demandez alors à l'équipe de Picasso96 ou au fabricant" 
					"de votre carte de développer le pilote adéquat."
				)
			)
	
		; Monitor
			(set #15kHzMonitor "[2p15 kHz")
			(set #31kHzMonitor "31 kHz")
